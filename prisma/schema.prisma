datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id             String  @id @default(uuid())
  name           String
  description    String
  price          Float
  rating         Int
  category       String
  tier           String
  image          String
  additionalInfo String? // Make this optional
  review         String? // Make this optional
}
